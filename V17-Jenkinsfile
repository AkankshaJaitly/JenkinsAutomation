pipeline {
  agent any
    stages {
	    stage('First Stage') {
		    steps {
         // send build started notifications
         slackSend (color: '#00FF00', message: "STARTED: Job '${env.JOB_NAME} [${env.BUILD_NUMBER}]' (${env.BUILD_URL})")
			   echo "I am in First stage"
			}
		}
        stage('Second Stage') {
		    steps {
			   echo "I am in Second stage"
			}
		}
        stage('Pre Final Stage') {
		    steps {
			   echo "I am in Pre Final stage"
			}
		}
	      stage('Final Stage') {
		    steps {
			   echo "I am in Final stage"
			}
		}
    }
 }
}
